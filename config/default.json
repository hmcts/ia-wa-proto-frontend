{
  "useCSRFProtection": true,
  "security": {
    "referrerPolicy": "origin"
  },
  "session": {
    "useRedis": true,
    "useLogger": false,
    "redis": {
      "url": "redis://127.0.0.1:6379",
      "secret": "defaultsecret",
      "ttlInSeconds": 1200
    },
    "cookie": {
      "secret": "defaultcookiesecret",
      "maxAgeInMs": 1200000,
      "secure": false
    }
  }
}
